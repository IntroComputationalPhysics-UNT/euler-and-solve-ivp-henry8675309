        "# IC B\n",
        "tB, thB, vB = solve_pend(theta_0=0.0, v_0=2.0,\n",
        "                         method=\"DOP853\", rtol=1e-8, atol=1e-10)\n",
        "\n",
        "# θ(t) — assignment limits\n",
        "plt.figure(figsize=(10,2.6))\n",
        "plt.plot(tB, thB, label='solve_ivp (DOP853)')\n",
        "set_theta_time_axes(); plt.legend(); plt.tight_layout(); plt.show()\n",
        "\n",
        "# (optional) zoomed θ(t)\n",
        "plt.figure(figsize=(10,2.6))\n",
        "plt.plot(tB, thB)\n",
        "plt.xlim([0, 10])\n",
        "pad = 0.2\n",
        "plt.ylim([thB.min()-pad, thB.max()+pad])\n",
        "plt.xlabel('t'); plt.ylabel(r'$\\theta$'); plt.tight_layout(); plt.show()\n",
        "\n",
        "# Phase portrait (v vs theta) — assignment limits\n",
        "plt.figure(figsize=(12,3.2))\n",
        "plt.plot(thB, vB, label='solve_ivp')\n",
        "set_phase_axes(); plt.legend(); plt.tight_layout(); plt.show()\n",
        "\n",
        "# (optional) prettier phase portrait by wrapping theta into [-pi, pi]\n",
        "wrap_pi = np.vectorize(lambda x: (x + math.pi) % (2*math.pi) - math.pi)\n",
        "plt.figure(figsize=(12,3.2))\n",
        "plt.plot(wrap_pi(thB), vB, label='solve_ivp (wrapped θ)')\n",
        "plt.xlim([-math.pi-0.5, math.pi+0.5]); plt.ylim([-math.pi-0.5, math.pi+0.5])\n",
        "plt.xlabel(r'$\\theta$ (wrapped)'); plt.ylabel(r'$d\\theta/dt$'); plt.legend()\n",
        "plt.tight_layout(); plt.show()\n"
      ],
